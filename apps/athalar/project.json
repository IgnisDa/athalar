{
  "projectType": "application",
  "sourceRoot": "apps/athalar/src",
  "targets": {
    "build": {
      "executor": "@ignisda/nx-rust:build",
      "options": {
        "release": false
      },
      "configurations": {
        "production": {
          "release": true
        }
      }
    },
    "run": {
      "executor": "@ignisda/nx-rust:build",
      "options": {
        "release": false,
        "run": true
      }
    },
    "test": {
      "executor": "@ignisda/nx-rust:nextest",
      "options": {}
    },
    "lint": {
      "executor": "@ignisda/nx-rust:clippy",
      "options": {
        "fix": false,
        "failOnWarnings": true,
        "noDeps": true
      }
    }
  },
  "tags": []
}
